{
  "schemaVersion": 1,
  "id": "random_teleport",
  "version": "${version}",
  "name": "Random Teleport",
  "description": "Modification for minecraft allows you to teleport to a random location in the world.",
  "authors": [
    "MairwunNx (Pavel Erokhin)"
  ],
  "contact": {
    "homepage": "https://github.com/MairwunNx/RandomTeleport",
    "sources": "https://github.com/MairwunNx/RandomTeleport/tree/FABRIC-1.14.4"
  },
  "license": "GNU GPLv3",
  "icon": "assets/randomteleport/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.mairwunnx.randomteleport.EntryPoint"
      }
    ]
  },
  "requires": {
    "fabric-language-kotlin": ">=1.3.71"
  },
  "depends": {
    "fabricloader": ">=0.8.1",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.14.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
